[application]

config/name="Chain3"
run/main_scene="res://scenes/GameWindow.tscn"
config/icon="res://icon.png"

[autoload]

IconLoader="*res://addons/file-editor/scripts/IconLoader.gd"
LastOpenedFiles="*res://addons/file-editor/scripts/LastOpenedFiles.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/test_width=576
window/size/test_height=1024
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "file-editor" )

[input]

ui_touch={
